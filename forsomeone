    Name = "Delete Advertisement Decals",
    Callback = function()
            if not game.Players.LocalPlayer.Character:FindFirstChild("F3X Btools!") then game.Players.LocalPlayer.Backpack["F3X Btools!"].Parent = game.Players.LocalPlayer.Character end
